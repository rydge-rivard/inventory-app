<!DOCTYPE html>
<html>
  <head>
    <%- include('../views/components/head'); %>
  </head>
  <header><%- include('../views/components/header'); %></header>
  <body>
    <main>
      <h1>Parts by Vehicle</h1>
      <button onclick="location.href='/catalog/vehicle_list/create'">
        Create Vehicle
      </button>
      <section>
        <ul class="part-ul">
          <% vehicle_list.forEach(function(vehicle) { %>
          <li>
            <div class="part-card">
              <ul>
                <a class="detail-link" href="<%= vehicle.url %>">
                  <li>
                    <strong
                      ><%= vehicle.manufacturer + ' ' + vehicle.model%></strong
                    >
                  </li>
                </a>
              </ul>
            </div>
          </li>
          <% }); %>
        </ul>
      </section>
    </main>
  </body>
</html>
